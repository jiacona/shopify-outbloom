    "rich_text_4eVbxA": {
      "type": "rich-text",
      "blocks": {
        "heading_iEVKYX": {
          "type": "heading",
          "settings": {
            "heading": "Through the year",
            "heading_size": "h1"
          }
        },
        "text_FMJ3nh": {
          "type": "text",
          "settings": {
            "text": "<p>All our flowers are grown with the seasons. This means what is available changes over the course of the year as different flowers come and go. To get a sense of what is at its peak at what point, have a nose through our seasonal galleries.<\/p>"
          }
        }
      },
