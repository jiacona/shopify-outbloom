    "rich_text_4eVbxA": {
      "type": "rich-text",
      "blocks": {
        "heading_iEVKYX": {
          "type": "heading",
          "settings": {
            "heading": "Through the year",
            "heading_size": "h1"
          }
        },
        "text_FMJ3nh": {
          "type": "text",
          "settings": {
            "text": "<p>All our flowers are grown with the seasons. This means what is available changes over the course of the year as different flowers come and go.<\/p><p>To keep up to date, why not sign up to our farm newsletter? We send them out every month or two. In them you can expect to hear what flowers we are currently  excited about, news from the farm, events we'll be at, and even the occasional special offer just for subscribers!<\/p>"
          }
        },
        "caption_q6CcMJ": {
          "type": "caption",
          "settings": {
            "caption": "Just pop your email address in the box and we'll get you signed up.",
            "text_style": "subtitle",
            "text_size": "medium"
          }
        }
      },
      "block_order": [
