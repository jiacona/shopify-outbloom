    "rich_text": {
      "type": "rich-text",
      "blocks": {
        "heading": {
          "type": "heading",
          "settings": {
            "heading": "Sensational flowers grown in West Wales",
            "heading_size": "h1"
          }
        },
        "text": {
          "type": "text",
          "settings": {
            "text": "<p>We grow top quality flowers without chemicals and in a way that encourages all kinds of life on the farm. We provide flowers in an ever changing selection throughout the growing season in the Lampeter area and beyond. They are alway fresh, scented, and fabulous.<\/p>"
          }
        },
        "button_AfL6Ci": {
          "type": "button",
          "settings": {
            "button_label": "Buy flowers",
            "button_link": "shopify:\/\/collections\/fresh-flowers",
